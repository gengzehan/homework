#include<stdio.h>

int choice(char way,double a,double b)
{
    int re;
    switch(way)
    {
        case '+':re=a+b;break;
        case '-':re=a-b;break;
        case '*':re=a*b;break;
        case '/':re=a/b;break;
    }
    return re;

}

void cut(char *p)
{
    char a[3],i=0,j=0,flag=0;
    double b[4],re,hp;
    while(*p!='\0')
    {
        if(*p<'0'||*p>'9')
        {
            a[j]=*p;
            p++;
            j++;

        }
        else
        {
            if(*(++p)>='0'&&*p<='9')
            {
                b[i]=(*(--p)-48)*10+(*(++p)-48);
                i++;
            }
            else
            {
                p--;
                b[i]=(*p)-48;
                i++;
            }
            p++;
        }
    }

    for(i=0;i<3;i++)
    {
        if(a[i]=='/'||a[i]=='*')
        {
            flag=1;
            re=choice(a[i],b[i],b[i+1]);
            if(i==0)
            {
                if(a[i+2]=='/'||a[i+2]=='*')
                {

                    hp=choice(a[i+2],b[2],b[3]);
                    re=choice(a[1],re,hp);
                    break;
                }
                else
                {
                    re=choice(a[1],re,b[2]);
                    re=choice(a[2],re,b[3]);
                }

            }
            if(i!=0)
            {
                if(i==1)
                 {
                    if(a[2]=='/'||a[2]=='*')
                    {
                        re=choice(a[2],re,b[3]);
                        re=choice(a[0],b[0],re);
                        break;
                    }
                    else
                    {
                        re=choice(a[0],b[0],re);
                        re=choice(a[2],re,b[3]);
                        break;
                    }
                 }
                 if(i==2)
                 {
                    hp=choice(a[0],b[0],b[1]);
                    re=choice(a[1],hp,re);
                    break;
                 }
            }
        }
    }
    if(flag==0)
    {
        re=b[0];
        for(i=0;i<3;i++)
        {
            re=choice(a[i],re,b[i+1]);
        }
    }
    printf("%f",re);
}

int main()
{
    char str[30],i;
    gets(str);
    cut(str);
}
